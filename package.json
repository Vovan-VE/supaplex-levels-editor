{
  "name": "sp-ed",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "npm ls husky > /dev/null && husky install || exit 0",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "tsc && cross-env CI=true react-scripts test --env=./jest-env-jsdom-ext.js",
    "test:watch": "react-scripts test --env=./jest-env-jsdom-ext.js",
    "typescript": "tsc"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^12.8.3",
    "@types/file-saver": "^2.0.3",
    "@types/jest": "^27.0.2",
    "@types/node": "^14.17.33",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "file-saver": "^2.0.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0-next.47",
    "sass": "^1.43.4",
    "typescript": "~4.4.4",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "prettier": "2.4.1"
  },
  "engines": {
    "node": ">=14"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "<rootDir>/.*\\.dev/.*"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md,html}": [
      "prettier --write"
    ]
  }
}
